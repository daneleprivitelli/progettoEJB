package Beans;

import javax.ejb.LocalBean;
import javax.ejb.Stateless;

import model.session.Docente;
import service.DocenteService;

/**
 * Session Bean implementation class SessionBean
 */
@Stateless
@LocalBean
public class SessionBean implements SessionBeanRemote, SessionBeanLocal {

    /**
     * Default constructor. 
     */
	
	
	
    public SessionBean() {
        
    }
    
    public Docente creoDocente(Integer iddocente, string nomedocente, string cognomedocente, Integer matricola ) {
    	DocenteService.insertDocente();
    }
    
    
    
    public String InsertDocente() {
		return null;
    	
    }
}
